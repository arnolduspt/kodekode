<script>
	  //<![CDATA[
	  /* replace img url by @arnolduspt */
    var x = document.getElementsByTagName('img');
for (var y = 0; y < x.length; y++) {
    x[y].src = x[y].src.replace(/[0-4]+.bp/, '3.bp');
} //]]>
	</script>

